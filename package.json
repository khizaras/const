{
  "name": "procore",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"npm run client:watch\" \"npm run server:dist\"",
    "start": "node server/src/index.js",
    "build": "npm run client:build",
    "server:dev": "nodemon server/src/index.js",
    "server:dist": "cross-env NODE_ENV=production nodemon --watch server/src --ext js,json --ignore dist --ignore client --ignore client/dist server/src/index.js",
    "client:build": "webpack --config client/webpack.config.js --mode production",
    "client:watch": "webpack --config client/webpack.config.js --mode development",
    "lint": "eslint client/src server/src --ext .js,.jsx",
    "lint:fix": "eslint client/src server/src --ext .js,.jsx --fix",
    "test:smoke": "node server/scripts/ci_smoke_test.js",
    "test:ci": "node server/scripts/ci_smoke_test.js",
    "test:inbound": "node server/scripts/test_inbound_email.js",
    "test:rfis": "node server/scripts/test_rfis_api.js",
    "seed:test": "node server/scripts/seed_test_data.js",
    "seed:issues": "node server/scripts/seed_issues_test_data.js",
    "seed:dailylogs": "node server/scripts/seed_daily_logs_test_data.js",
    "db:apply:issues": "node server/scripts/db_apply_sql.js server/db/issues.sql",
    "db:apply:dailylogs": "node server/scripts/db_apply_sql.js server/db/daily_logs.sql",
    "test:issues": "node server/scripts/test_issues_api.js",
    "test:dailylogs": "node server/scripts/test_daily_logs_api.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.5",
    "antd": "^5.19.0",
    "axios": "^1.7.2",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "^1.11.11",
    "dotenv": "^16.4.5",
    "express": "^4.22.1",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "multer": "^2.0.2",
    "mysql2": "^3.11.0",
    "node-cron": "^3.0.3",
    "nodemailer": "^7.0.11",
    "pino": "^9.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-redux": "^9.1.2",
    "react-router-dom": "^6.24.0",
    "sharp": "^0.34.5",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/eslint-parser": "^7.28.5",
    "@babel/preset-env": "^7.25.4",
    "@babel/preset-react": "^7.24.7",
    "@eslint/js": "^9.39.2",
    "babel-loader": "^9.1.3",
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "css-loader": "^7.1.2",
    "dotenv-webpack": "^8.1.0",
    "eslint": "^9.39.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "globals": "^16.5.0",
    "html-webpack-plugin": "^5.6.0",
    "less": "^4.2.0",
    "less-loader": "^12.2.0",
    "nodemon": "^3.1.4",
    "pino-pretty": "^11.2.2",
    "style-loader": "^4.0.0",
    "webpack": "^5.93.0",
    "webpack-cli": "^5.1.4"
  }
